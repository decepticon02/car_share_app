<div class="container mt-4"></div>
<% vozaci.forEach(function(usr) { %>
<div class="card mt-4" id="card-<%= usr.id%>">
    <div class="card-body">
      <h5 class="card-title">Username: <%= usr.usrname></h5>
      <p class="card-text">
        Email: <%=usr.Email%>
       </p>
      <p class="card-text">
        Password: <%=usr.Password%>
       </p>
      
      <form class="form-inline">
        <button class="btn btn-outline-success" type="button">Измени корисника</button>
        <button class="btn btn-outline-success" type="button">Обриши корисника</button>
      </form>
    </div>
</div>
<% }) %>
</div>